{
    "manifest_version": 3,
    "name": "NSFW Blocker",
    "description": "Filters NSFW images",
    "version": "0.1",
    "action": {
        "default_popup": "index.html",
        "default_icon": "no_under_18.png"
    },
    "background": {
        "service_worker": "scripts/intercept_nsfw_background.js",
        "type": "module"
    },
    "permissions": [
        "storage",
        "webRequest",
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>",
        "https://youtubenotifier.com:8000/*"
    ]
}